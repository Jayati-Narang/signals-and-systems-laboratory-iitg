var answers = ["3","1","3","3","2","3","2","2","1"];



if(ans1==answers[0]) {

	$( "#log" ).html( ans1 + " is collect!" );
	/*$('#log').addClass('right');*/
	$( "#log" ).prepend('<img id="theImg" src="images/tick.gif.jpg" />');
}
else if(ans1!=answers[0])
{
   $( "#log" ).prepend('<img id="theImg" src="images/wrong.gif.jpg" />');
}
if(ans2==answers[1])
{
